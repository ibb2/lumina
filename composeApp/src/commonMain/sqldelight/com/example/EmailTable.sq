CREATE TABLE Email(
    id INTEGER NOT NULL PRIMARY KEY,
    from_user TEXT NOT NULL,
    to_user TEXT,
    cc TEXT,
    bcc TEXT,
    subject TEXT ,
    body TEXT NOT NULL,
    account TEXT NOT NULL,
    FOREIGN KEY(account) REFERENCES Account(emailAddress) ON DELETE CASCADE
);

insertEmail:
INSERT INTO Email(id, from_user, to_user, cc, bcc, subject, body, account) VALUES(?, ?, ?, ?, ?, ?, ?, ?);

removeAllEmails:
DELETE FROM Email;

selectAllEmails:
SELECT * FROM Email;